#!/bin/sh
#
# Copyright (C) 2020 OpenWrt.org
#

. /lib/functions.sh
. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
	asrock,g10 |\
	nec,wg2600hp |\
	compex,wpq864 |\
	netgear,d7800 |\
	netgear,r7500 |\
	netgear,r7500v2 |\
	qcom,ipq8064-ap148 |\
	tplink,vr2600v |\
	linksys,ea7500-v1 |\
	linksys,ea8500 |\
	netgear,r7800 |\
	netgear,xr500 |\
	tplink,ad7200 |\
	tplink,c2600 |\
	zyxel,nbg6817 |\
	nec,wg2600hp3 |\
	buffalo,wxr-2533dhp |\
	qcom,ipq8064-db149 |\
	qcom,ipq8064-ap161)
		ucidef_set_compat_version "1.1"
		;;
esac

board_config_flush

exit 0
